{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "string"
    },
    "cod": {
      "type": "string"
    },
    "count": {
      "type": "integer"
    },
    "list": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "name": {
              "type": "string"
            },
            "coord": {
              "type": "object",
              "properties": {
                "lon": {
                  "type": "number"
                },
                "lat": {
                  "type": "number"
                }
              }
            },
            "main": {
              "type": "object",
              "properties": {
                "temp": {
                  "type": "number"
                },
                "feels_like": {
                  "type": "number"
                },
                "temp_min": {
                  "type": "number"
                },
                "temp_max": {
                  "type": "number"
                },
                "pressure": {
                  "type": "integer"
                },
                "humidity": {
                  "type": "integer"
                }
              }
            },
            "dt": {
              "type": "integer"
            },
            "wind": {
              "type": "object",
              "properties": {
                "speed": {
                  "type": "number"
                },
                "deg": {
                  "type": "integer"
                }
              }
            },
            "sys": {
              "type": "object",
              "properties": {
                "country": {
                  "type": "string"
                }
              }
            },
            "rain": {
              "type": "object",
              "properties": {
                "1h": {
                  "type": "number"
                }
              }
            },
            "snow": {
              "type": "null"
            },
            "clouds": {
              "type": "object",
              "properties": {
                "all": {
                  "type": "number"
                }
              }
            },
            "weather": {
              "type": "array",
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer"
                    },
                    "main": {
                      "type": "string"
                    },
                    "description": {
                      "type": "string"
                    },
                    "icon": {
                      "type": "string"
                    }
                  }
                }
              ]
            }
          }
        }
      ]
    }
  }
}